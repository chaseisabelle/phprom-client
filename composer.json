{
  "name": "chaseisabelle/phprom-client",
  "description": "phprom client",
  "type": "library",
  "keywords": [
    "prometheus",
    "phprom"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "chase isabelle"
    }
  ],
  "require": {
    "php": ">=7.1",
    "google/protobuf": "3.13.*",
    "grpc/grpc": "1.30.0"
  },
  "suggest": {
    "ext-grpc": "* required for grpc api",
    "ext-curl": "* required for rest/http api"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "PHProm\\": "src/",
      "PHProm\\V1\\": "src/pb/PHProm/V1/",
      "GPBMetadata\\": "src/pb/GPBMetadata/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHProm\\Test\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
